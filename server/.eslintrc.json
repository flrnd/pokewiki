{
  "plugins": ["graphql"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "graphql/named-operations": "error",
    "graphql/capitalized-type-name": "error",
    "graphql/no-deprecated-fields": "error",
    "graphql/template-strings": [
      "error",
      {
        "env": "literal",
        "tagName": "gql"
      }
    ]
  }
}
